
@model IEnumerable<SetServiceEnd.Areas.Admin.Models.ServiceViewModel>

@{
    ViewBag.Title = "خدمات";
    Layout = "~/Views/Shared/_Managers_Layout.cshtml";
}

<h2>لسیت همه خدمات</h2>

<table id="usersTable" class="table table-striped table-bordered" cellspacing="0" width="100%">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(m => m.Name)</th>
            <th>@Html.DisplayNameFor(m => m.Intro)</th>
            <th>@Html.DisplayNameFor(m => m.Company)</th>
            <th>@Html.DisplayNameFor(m => m.Category)</th>
            <th>آدرس</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>>@item.Intro</td>
                <td>>@item.Company</td>
                <td>>@item.Category</td>
                <td>
                    <a href="@item.Url" class="btn btn-primary" target="_blank">@item.Url</a>
                </td>
            </tr>
        }
    </tbody>
</table>

